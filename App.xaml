<?xml version="1.0" encoding="utf-8" ?>
<Application
    x:Class="NeoBox.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:NeoBox"
    xmlns:wuc="using:WinUICommunity">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///WinUICommunity.Components/Themes/Generic.xaml" />
                <ResourceDictionary Source="ms-appx:///WinUICommunity.LandingPages/Themes/Generic.xaml" />
                <wuc:ItemTemplates />
                <ResourceDictionary Source="Themes/ThemeResources.xaml" />
                <ResourceDictionary Source="Themes/Fonts.xaml" />
                <ResourceDictionary Source="Themes/Converters.xaml" />
                <ResourceDictionary Source="Themes/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <!--  Other app resources here  -->

            <Thickness x:Key="NavigationViewHeaderMargin">0,0,0,0</Thickness>

        </ResourceDictionary>
    </Application.Resources>
</Application>
